{
  "name": "katm/katm-sdk-laravel",
  "description": "Comprehensive Laravel SDK for the KATM Service API.",
  "type": "library",
  "license": "MIT",
  "keywords": ["katm", "katm-sdk-laravel", "katm-package"],
  "require": {
    "php": "^8.2",
    "illuminate/support": "^10.0 || ^11.0",
    "illuminate/http": "^10.0 || ^11.0",
    "spatie/laravel-data": "^4.0|^5.0"
  },
  "require-dev": {
    "orchestra/testbench": "^8.0 || ^9.0",
    "phpunit/phpunit": "^10.0 || ^11.0",
    "mockery/mockery": "^1.6",
    "fakerphp/faker": "^1.23",
    "laravel/pint": "^1.24"
  },

  "autoload": {
    "psr-4": {
      "Katm\\KatmSdk\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Katm\\KatmSdk\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Katm\\KatmSdk\\Providers\\KatmSdkServiceProvider"
      ],
      "aliases": {
        "Katm": "Katm\\KatmSdk\\Facades\\Katm"
      }
    }
  },
  "authors": [
    {
      "name": "Diyorbek Abdumutalibov",
      "email": "diordev9@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "prefer-stable": true
}
